# Video Call Server Configuration

# Service
NODE_ENV=development
PORT=8080
API_URL=https://videocall.loctelli.com

# Database (can be shared with main CRM or separate)
DATABASE_URL=postgresql://postgres:password@localhost:5432/loctelli_videocall

# Redis
REDIS_URL=redis://localhost:6379

# LiveKit Configuration
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret
LIVEKIT_WS_URL=wss://your-livekit-instance.livekit.cloud

# AI Services
OPENAI_API_KEY=sk-xxx
DEEPGRAM_API_KEY=xxx
ELEVENLABS_API_KEY=xxx

# Main CRM Integration
LOCTELLI_CRM_API_URL=https://loctelli.com/api
LOCTELLI_CRM_API_KEY=your_crm_api_key
LOCTELLI_CRM_WEBHOOK_SECRET=your_webhook_secret

# Storage (for recordings)
R2_ACCOUNT_ID=xxx
R2_ACCESS_KEY_ID=xxx
R2_SECRET_ACCESS_KEY=xxx
R2_BUCKET_NAME=loctelli-videocall-recordings

# Authentication
JWT_SECRET=your-video-call-jwt-secret
API_KEY_ENCRYPTION_KEY=your-encryption-key-32-chars

# Monitoring
SENTRY_DSN=https://xxx@sentry.io/xxx
LOG_LEVEL=info
